{
    "name": "seiffert/crowd-rest-bundle",
    "description": "This bundle provides a simple API for Atlassian Crowd's REST API.",
    "version": "1.0.0",
    "license": "CC-BY-SA-3.0",
    "autoload": {
        "psr-0": { "Seiffert\\CrowdRestBundle": "" }
    },
    "target-dir": "Seiffert/CrowdRestBundle",
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": ">=2.2.0,<2.3",
        "guzzle/guzzle": "*"
    },
    "require-dev": {
        "phploc/phploc": "*",
        "phpmd/phpmd": "1.4.0",
        "pdepend/pdepend": "1.1.0",
        "squizlabs/php_codesniffer": "*",
        "phpunit/phpunit": "3.7.*",
        "phpunit/php-code-coverage": "1.2.*",
        "phpunit/php-timer": "1.0.4"
    }
}
